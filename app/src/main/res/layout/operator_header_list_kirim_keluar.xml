<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.operator.suratkeluar.TambahArsipKeluarFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/kategori"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:enabled="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:id="@+id/kategori_et"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:padding="5dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/perincian"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:enabled="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:id="@+id/perincian_et"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:padding="5dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/nomor_surat"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:enabled="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:id="@+id/nomor_et"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:padding="5dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor" />

    <TextView
        android:id="@+id/kepada_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/kepada"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:enabled="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/kepada_et"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:padding="5dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor"
        android:inputType="textMultiLine" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dari"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:enabled="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/dari_et"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:padding="5dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor"
        android:inputType="textMultiLine" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tanggal_surat"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:enabled="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:id="@+id/tanggal_et"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:padding="5dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/perihal"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:enabled="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:id="@+id/perihal_et"
        android:paddingBottom="5dp"
        android:paddingHorizontal="5dp"
        android:paddingTop="5dp"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/lampiran"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:enabled="false"
        android:id="@+id/lampiran_et"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:padding="5dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/file"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:enabled="false"
        android:id="@+id/file_et"
        android:layout_marginHorizontal="7dp"
        android:layout_marginBottom="10dp"
        android:padding="5dp"
        android:background="@drawable/text_box_light"
        tools:ignore="Autofill,LabelFor" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/diteruskan_kepada"
        android:textStyle="bold"
        android:layout_marginStart="7dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="4dp"
        android:textColor="@color/black"
        android:textSize="16sp"/>

    <LinearLayout
        android:id="@+id/pilih_semua_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <CheckBox
            android:id="@+id/header_checkbox_item"
            android:layout_width="25dp"
            android:layout_height="25dp" />

        <TextView
            android:id="@+id/header_text_item_checkbox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:layout_marginHorizontal="6dp"
            android:layout_marginBottom="15dp"
            android:text="@string/pilih_semua"/>

    </LinearLayout>

</LinearLayout>